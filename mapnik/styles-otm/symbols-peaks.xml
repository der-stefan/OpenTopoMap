<Style name="symbols-peaks">
    <Rule>
		&maxscale_zoom7;
		&minscale_zoom7;
        <Filter>(([natural] = 'peak' or [natural] = 'volcano') and ([otm_isolation] = 100000))</Filter>
        <MarkersSymbolizer placement="point" ignore-placement="true" file="symbols-otm/peak_z8.png"/>
	</Rule>
    <Rule>
		&maxscale_zoom8;
		&minscale_zoom8;
        <Filter>(([natural] = 'peak' or [natural] = 'volcano') and ([otm_isolation] &gt; 75000))</Filter>
        <MarkersSymbolizer placement="point" ignore-placement="true" file="symbols-otm/peak_z8.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom9;
        <Filter>(([natural] = 'peak' or [natural] = 'volcano') and ([otm_isolation] &gt; 20000))</Filter>
        <MarkersSymbolizer placement="point" ignore-placement="true" file="symbols-otm/peak_z8.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
        <Filter>(([natural] = 'peak' or [natural] = 'volcano') and ([otm_isolation] &gt; 8000))</Filter>
        <MarkersSymbolizer placement="point" ignore-placement="true" file="symbols-otm/peak_z8.png"/>
	</Rule>
	<Rule>
	 	&maxscale_zoom11;
 		&minscale_zoom11;
        <Filter>(([natural] = 'peak' or [natural] = 'volcano') and ([otm_isolation] &gt;  3000))</Filter>
        <MarkersSymbolizer placement="point" ignore-placement="true" file="symbols-otm/peak_z8.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom12;
        <Filter>(([natural] = 'peak' or [natural] = 'volcano') and ([otm_isolation] &gt;  1200))</Filter>
        <MarkersSymbolizer placement="point" ignore-placement="true" file="symbols-otm/peak.png"/>
	</Rule>
    <Rule>
		&maxscale_zoom13;
		&minscale_zoom13;
		<Filter>(([natural] = 'peak' or [natural] = 'volcano') and (not [summit:cross] = 'yes') and ([otm_isolation] &gt; 500))</Filter>
        <MarkersSymbolizer placement="point" ignore-placement="true" file="symbols-otm/peak.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom13;
        <Filter>(([natural] = 'peak' or [natural] = 'volcano') and ([summit:cross] = 'yes') and ([otm_isolation] &gt; 300))</Filter>
        <MarkersSymbolizer placement="point" ignore-placement="true" file="symbols-otm/summit_cross.png" transform="translate(0 -3)"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom17;
		<Filter>(([natural] = 'peak' or [natural] = 'volcano') and (not [summit:cross] = 'yes'))</Filter>
        <MarkersSymbolizer placement="point" ignore-placement="true" file="symbols-otm/peak.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom17;
        <Filter>(([natural] = 'peak' or [natural] = 'volcano') and ([summit:cross] = 'yes'))</Filter>
        <MarkersSymbolizer placement="point" ignore-placement="true" file="symbols-otm/summit_cross.png" transform="translate(0 -3)"/>
	</Rule>

</Style>
