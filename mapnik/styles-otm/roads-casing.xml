    <!-- ferratas are rendered like visible T5-6 -->
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom14;
		<Filter>([highway] = 'via_ferrata') or (([via_ferrata_scale] != null and [via_ferrata_scale] != '0') and ([highway] = 'path' or [highway] = 'footway'))</Filter>
