<Style name="cliffs">
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom13;
		<Filter>([natural] = 'cliff') or ([man_made] = 'embankment')</Filter>
		<LinePatternSymbolizer file="symbols-otm/cliff_z13.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom14;
		<Filter>([natural] = 'cliff') or ([man_made] = 'embankment')</Filter>
		<LinePatternSymbolizer file="symbols-otm/cliff_z14.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom17;
		<Filter>([natural] = 'cliff') or ([man_made] = 'embankment')</Filter>
		<LinePatternSymbolizer file="symbols-otm/cliff_z15.png"/>
	</Rule>

	<Rule>
		&maxscale_zoom14;
		&minscale_zoom14;
		<Filter>([natural] = 'crevasse')</Filter>
                <LineSymbolizer stroke="#000088" stroke-width="0.5" stroke-opacity="0.05"/>
	</Rule>

	<Rule>
		&maxscale_zoom15;
		&minscale_zoom15;
		<Filter>([natural] = 'crevasse')</Filter>
                <LineSymbolizer stroke="#000088" stroke-width="0.5" stroke-opacity="0.1"/>
	</Rule>


	<Rule>
		&maxscale_zoom16;
		&minscale_zoom17;
		<Filter>([natural] = 'crevasse')</Filter>
                <LineSymbolizer stroke="#000088" stroke-width="0.25" stroke-opacity="0.2"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom15;
		<Filter>[barrier] = 'ditch'</Filter>
		<LinePatternSymbolizer file="symbols-otm/ditch_z14.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom17;
		<Filter>[barrier] = 'ditch'</Filter>
		<LinePatternSymbolizer file="symbols-otm/ditch_z16.png"/>
	</Rule>

	<Rule>
		&maxscale_zoom13;
		&minscale_zoom14;
		<Filter>([man_made] = 'dyke' or [barrier] = 'embankment')</Filter>
		<LineSymbolizer stroke="black" stroke-width="0.5"/>
		<LinePatternSymbolizer file="symbols-otm/dyke.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom17;
		<Filter>([man_made] = 'dyke' or [barrier] = 'embankment') and ([highway] = null)</Filter>
		<LineSymbolizer stroke="black" stroke-width="0.5"/>
		<LinePatternSymbolizer file="symbols-otm/dyke.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom17;
		<Filter>([man_made] = 'dyke') and ([highway] = 'path')</Filter>
		<LineSymbolizer stroke="grey" stroke-width="1"/>
		<LinePatternSymbolizer file="symbols-otm/dyke_path.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom17;
		<Filter>([man_made] = 'dyke') and ([highway] = 'track')</Filter>
		<LineSymbolizer stroke="grey" stroke-width="1"/>
		<LinePatternSymbolizer file="symbols-otm/dyke_track.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom17;
		<Filter>([man_made] = 'dyke') and not (([highway] = 'path') or ([highway] = 'track') or ([highway] = null))</Filter>
		<LineSymbolizer stroke="grey" stroke-width="1"/>
		<LinePatternSymbolizer file="symbols-otm/dyke_track.png"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom17;
		<Filter>([man_made] = 'breakwater')</Filter>
		<LineSymbolizer stroke="#505050" stroke-width="0.8"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom17;
		<Filter>(([man_made] = 'pier') and ([area] = 'no'))</Filter>
		<LineSymbolizer stroke="#505050" stroke-width="1"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom17;
		<Filter>(([man_made] = 'pier') and ([area] = 'yes'))</Filter>
		<PolygonSymbolizer fill="white" />
		<LineSymbolizer stroke="#505050" stroke-width="1"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom17;
		<Filter>([man_made] = 'groyne')</Filter>
		<LineSymbolizer stroke="#505050" stroke-width="0.8"/>
	</Rule>
</Style>
