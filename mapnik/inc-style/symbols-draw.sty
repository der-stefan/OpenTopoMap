<Style name="symbols-draw">
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>( ([man_made] = 'tower') and ( 
			not (([tower:type] = 'communication') or ([tower:type] = 'observation'))))</Filter>
		<PointSymbolizer file="symbols-topo/tower.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>([man_made] = 'chimney')</Filter>
		<PointSymbolizer file="symbols-topo/chimney.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom16;
		<Filter>([man_made] = 'lighthouse')</Filter>
		<PointSymbolizer file="symbols-topo/lighthouse.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>( ([man_made] = 'tower') and (([tower:type] = 'observation') or ([tourism] = 'viewpoint')))</Filter>
		<PointSymbolizer file="symbols-topo/observation_tower.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom16;
		<Filter>( ([man_made] = 'tower') and (([tower:type] = 'observation') or ([tourism] = 'viewpoint')))</Filter>
		<TextSymbolizer face-name="DejaVu Sans Oblique" size="9" fill="black" halo-radius="1" halo-fill="rgba(255,255,255,0.7)" placement="point" dy="11">[name]</TextSymbolizer>
	</Rule>

	<Rule>
		&maxscale_zoom11;
		&minscale_zoom16;
		<Filter>([man_made] = 'communications_tower') or ([communication:radio] = 'yes') or ([communication:television] = 'yes')</Filter>
		<PointSymbolizer file="symbols-topo/communications_tower.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom16;
		<Filter>([man_made] = 'communications_tower') or ([communication:radio] = 'yes') or ([communication:television] = 'yes')</Filter>
		<TextSymbolizer face-name="DejaVu Sans Oblique" size="9" fill="black" halo-radius="1" halo-fill="rgba(255,255,255,0.9)" placement="point" dy="11">[name]</TextSymbolizer>
	</Rule>

	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>[man_made] = 'tower' and [tower:type] = 'communication'</Filter>
		<PointSymbolizer file="symbols-topo/communications_tower.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom16;
		<Filter>([man_made] = 'mast')</Filter>
		<PointSymbolizer file="symbols-topo/communications_mast.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>([man_made] = 'water_tower')</Filter>
		<PointSymbolizer file="symbols-topo/water_tower.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom16;
		<Filter>([man_made] = 'watermill')</Filter>
		<PointSymbolizer file="symbols-topo/watermill.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom16;
		<Filter>([man_made] = 'water_tower')</Filter>
		<TextSymbolizer face-name="DejaVu Sans Oblique" size="9" fill="black" halo-radius="2" halo-fill="rgba(255,255,255,0.7)" placement="point" dy="9">[name]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom16;
		<Filter>([natural] = 'spring')</Filter>
		<PointSymbolizer file="symbols-topo/spring.png"/>
	</Rule>

	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>([natural] = 'cave_entrance')</Filter>
		<PointSymbolizer file="symbols-topo/cave.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom16;
		<Filter>([natural] = 'cave_entrance')</Filter>
		<TextSymbolizer face-name="DejaVu Sans Oblique" size="9" fill="black" halo-radius="2" halo-fill="rgba(255,255,255,0.7)" placement="point" dy="6">[name]</TextSymbolizer>
	</Rule>

	<Rule>
		&maxscale_zoom14;
		&minscale_zoom16;
		<Filter>(([natural] = 'tree') and (not ([name] = null) or ([denotation] = 'landmark')) and (not ([type] = 'conifer')))</Filter>
		<PointSymbolizer file="symbols-topo/broadleaf_tree.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom16;
		<Filter>(([natural] = 'tree') and (not ([name] = null) or ([denotation] = 'landmark')) and ([type] = 'conifer'))</Filter>
		<PointSymbolizer file="symbols-topo/conifer_tree.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom16;
		<Filter>(([power] = 'generator') and ([generator:source] = 'wind'))</Filter>
		<PointSymbolizer file="symbols-topo/wind_turbine.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>(([power] = 'generator') and not ([generator:source] = 'wind' or [generator:source] = 'solar'))</Filter>
		<PointSymbolizer file="symbols-topo/power_station.png"/>
	</Rule>
 
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom16;
		<Filter>(([historic] = 'wayside_cross') or ([historic] = 'wayside_shrine'))</Filter>
		<PointSymbolizer file="symbols-topo/wayside_cross.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>([man_made] = 'cross')</Filter>
		<PointSymbolizer file="symbols-topo/cross.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom16;
		<Filter>(([historic] = 'memorial') or ([historic] = 'monument'))</Filter>
		<PointSymbolizer file="symbols-topo/monument.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom16;
		<Filter>(([historic] = 'archaeological_site') and ([site_type] = 'tumulus'))</Filter>
		<PointSymbolizer file="symbols-topo/tumulus.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>(([man_made] = 'mineshaft') or ([man_made] = 'adit')) and not (([disused] = 'yes') or ([abandoned] = 'yes'))</Filter>
		<PointSymbolizer file="symbols-topo/mineshaft.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>(([man_made] = 'mineshaft') or ([man_made] = 'adit')) and (([disused] = 'yes') or ([abandoned] = 'yes'))</Filter>
		<PointSymbolizer file="symbols-topo/mineshaft_disused.png"/>
	</Rule>

	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>([railway] = 'station')</Filter>
		<PointSymbolizer file="symbols-topo/station.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom16;
		<Filter>([railway] = 'station')</Filter>
		<TextSymbolizer face-name="DejaVu Sans Oblique" size="9" fill="#5427ff" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="6">[name]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
 		<Filter>([sport] = 'swimming') and not ([access] = 'private')</Filter>
		<PointSymbolizer file="symbols-topo/swimming.png"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom13;
 		<Filter>([tourism] = 'alpine_hut') or ([tourism] = 'wilderness_hut') </Filter>
		<PointSymbolizer file="symbols-topo/hut.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom16;
 		<Filter>([tourism] = 'alpine_hut') or ([tourism] = 'wilderness_hut') </Filter>
		<PointSymbolizer file="symbols-topo/hut.png"/>
		<TextSymbolizer face-name="DejaVu Sans Oblique" size="9" fill="black" halo-radius="2" halo-fill="rgba(255,255,255,0.7)" placement="point" dy="6">[name]</TextSymbolizer>
	</Rule>
	
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom16;
 		<Filter>([amenity] = 'shelter') </Filter>
		<PointSymbolizer file="symbols-topo/shelter.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom14;
 		<Filter>([tourism] = 'camp_site') </Filter>
		<PointSymbolizer file="symbols-topo/campsite_z13.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom16;
 		<Filter>([tourism] = 'camp_site') </Filter>
		<PointSymbolizer file="symbols-topo/campsite.png"/>
	</Rule>


<!-- POIs from zoom 16 -->
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom16;
 		<Filter>([amenity] = 'hospital') </Filter>
		<PointSymbolizer file="symbols-topo/hospital.png"/>
		<TextSymbolizer face-name="DejaVu Sans Oblique" size="9" fill="black" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="6">[name]</TextSymbolizer>
	</Rule>

<!--	<Rule>
		&maxscale_zoom16;
		&minscale_zoom16;
 		<Filter>([amenity] = 'pharmacy') </Filter>
		<PointSymbolizer file="symbols-topo/pharmacy.png"/>
	</Rule>
-->

	<Rule>
		&maxscale_zoom16;
		&minscale_zoom16;
 		<Filter>([amenity] = 'parking') and not ([access] = 'private' or [access] = 'restricted' or [access] = 'permissive' or [access] = 'customers' or [access] = 'no') </Filter>
		<PointSymbolizer file="symbols-topo/parking.png"/>
	</Rule>

<!-- <Rule>
		&maxscale_zoom16;
		&minscale_zoom16;
 		<Filter>([highway] = 'bus_stop') </Filter>
		<PointSymbolizer file="symbols-topo/bus_stop.png"/>
	</Rule>
-->

</Style>
